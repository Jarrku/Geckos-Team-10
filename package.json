{
  "name": "grillber",
  "version": "1.0.0",
  "description": "an uber for grilling. Chingu Voyage 4 team app. ",
  "main": "./server/server.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "babel-watch ./server/server.js",
    "client": "node ./server/start-client.js"
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "mongoose": "^5.0.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  }
}
